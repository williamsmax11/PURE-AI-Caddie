{
  "expo": {
    "name": "Pure",
    "slug": "pure-ai-caddie",
    "scheme": "com.puregolf.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/PureAppIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2d5016"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.puregolf.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Pure needs your location to provide accurate distance measurements and course information.",
        "NSLocationAlwaysUsageDescription": "Pure uses your location to track your position on the golf course.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.puregolf.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "ee82082a-27db-4da9-a12b-e8302523d9c7"
      }
    },
    "owner": "williamsmax",
    "plugins": [
      "expo-font",
      "expo-web-browser"
    ]
  }
}
