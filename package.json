  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@types/react": "~19.1.10",
    "babel-jest": "^29.6.3",
    "babel-preset-expo": "^54.0.10",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=18"
  }
